# PyPI Upload Configuration
# Copy this file to .env and fill in your actual tokens

# PyPI API Token (from https://pypi.org/manage/account/token/)
TWINE_USERNAME=__token__
TWINE_PASSWORD=pypi-your_actual_token_here

# TestPyPI API Token (from https://test.pypi.org/manage/account/token/)
TWINE_TEST_USERNAME=__token__
TWINE_TEST_PASSWORD=pypi-your_test_token_here

# GitHub Token (optional, for automated releases)
GITHUB_TOKEN=your_github_token_here